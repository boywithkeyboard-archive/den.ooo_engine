{
  "name": "den.ooo",
  "version": "1.0.0",
  "author": "Samuel Kopp (https://samuelkopp.de)",
  "license": "Apache-2.0",
  "description": "The sleek, feature-rich module registry for Deno. ðŸ“¦",
  "funding": "https://github.com/sponsors/boywithkeyboard",
  "repository": "github:dendotooo/den.ooo",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/index.d.ts",
    "./dist/index.mjs",
    "./dist/index.cjs",
    "./dist/resolvers/index.d.ts",
    "./dist/resolvers/index.mjs",
    "./dist/resolvers/index.cjs"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./resolvers": {
      "import": {
        "types": "./dist/resolvers/index.d.ts",
        "default": "./dist/resolvers/index.mjs"
      },
      "require": {
        "types": "./dist/resolvers/index.d.ts",
        "default": "./dist/resolvers/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "builder --esm --cjs && tsc",
    "test": "npm run build && tsc && node test.mjs"
  },
  "devDependencies": {
    "@boywithkeyboard/builder": "^0.4.2",
    "@gitbeaker/rest": "^39.25.0",
    "@sinclair/typebox": "^0.31.28",
    "@types/node": "^20.10.4",
    "lru-cache": "^10.1.0",
    "octokit": "^3.1.2",
    "semver": "^7.5.4",
    "typescript": "^5.3.3"
